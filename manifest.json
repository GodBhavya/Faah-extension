{
  "manifest_version": 3,
  "name": "Marks App Faah",
  "version": "1.0",
  "description": "Plays a sound on wrong answers.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["faah.mp3"],
      "matches": ["<all_urls>"]
    }
  ]
}